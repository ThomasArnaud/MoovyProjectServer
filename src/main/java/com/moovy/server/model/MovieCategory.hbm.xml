<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.moovy.server.model.MovieCategory" table="movie_category" schema="movie">
        <composite-id mapped="true" class="com.moovy.server.model.MovieCategoryPK">
            <key-property name="idMovie" column="id_movie"/>
            <key-property name="codeCategory" column="code_category"/>
        </composite-id>
    </class>
</hibernate-mapping>