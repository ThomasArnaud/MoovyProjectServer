<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.moovy.server.model.Character" table="character" schema="movie">
        <composite-id mapped="true" class="com.moovy.server.model.CharacterPK">
            <key-property name="idMovie" column="id_movie"/>
            <key-property name="idActor" column="id_actor"/>
        </composite-id>
        <property name="name" column="name"/>
    </class>
</hibernate-mapping>